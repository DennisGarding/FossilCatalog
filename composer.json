{
  "type": "project",
  "license": "MIT License",
  "version": "1.0.0-beta-1",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-yaml": "*",
    "ext-zip": "*",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.16",
    "dompdf/dompdf": "^2.0",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.24",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "7.0.*",
    "symfony/cache": "7.0.*",
    "symfony/console": "7.0.*",
    "symfony/doctrine-messenger": "7.0.*",
    "symfony/dotenv": "7.0.*",
    "symfony/expression-language": "7.0.*",
    "symfony/finder": "7.0.*",
    "symfony/flex": "^2",
    "symfony/form": "7.0.*",
    "symfony/framework-bundle": "7.0.*",
    "symfony/http-client": "7.0.*",
    "symfony/intl": "7.0.*",
    "symfony/mailer": "7.0.*",
    "symfony/mime": "7.0.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "7.0.*",
    "symfony/process": "7.0.*",
    "symfony/property-access": "7.0.*",
    "symfony/property-info": "7.0.*",
    "symfony/runtime": "7.0.*",
    "symfony/security-bundle": "7.0.*",
    "symfony/serializer": "7.0.*",
    "symfony/string": "7.0.*",
    "symfony/translation": "7.0.*",
    "symfony/twig-bundle": "7.0.*",
    "symfony/validator": "7.0.*",
    "symfony/web-link": "7.0.*",
    "symfony/yaml": "7.0.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "ext-pdo": "*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "fix-cs-dry": [
      "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --dry-run --diff --config=.php-cs-fixer.dist.php"
    ],
    "fix-cs": [
      "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config=.php-cs-fixer.dist.php"
    ],
    "phpunit": [
      "./vendor/phpunit/phpunit/phpunit"
    ],
    "phpstan": [
      "./vendor/bin/phpstan analyse"
    ],
    "setup": [
      "php bin/console app:create-env-file",
      "php bin/console app:create-database",
      "php bin/console doctrine:migrations:migrate -n",
      "php bin/console app:create-default-data",
      "php bin/console app:create-demo-user",
      "php bin/console app:install-translations",
      "@composer buildjs",
      "php bin/console app:create-lock-file",
      "php bin/console app:prepare-test-database"
    ],
    "demo": [
      "php bin/console app:demo"
    ],
    "translate": [
      "php bin/console app:translate"
    ],
    "buildjs": [
      "cd javascript; npm install; npm run build"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.0.*"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.50",
    "kubawerlos/php-cs-fixer-custom-fixers": "^3.21",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/phpstan-symfony": "^1.3",
    "phpunit/phpunit": "^9",
    "symfony/browser-kit": "7.0.*",
    "symfony/css-selector": "7.0.*",
    "symfony/debug-bundle": "7.0.*",
    "symfony/maker-bundle": "^1.51",
    "symfony/phpunit-bridge": "^6.3",
    "symfony/stopwatch": "7.0.*",
    "symfony/web-profiler-bundle": "7.0.*"
  }
}
